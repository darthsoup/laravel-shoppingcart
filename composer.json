{
    "name": "darthsoup/shoppingcart",
    "description": "Laravel 5 Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "Rob_Gloudemans@hotmail.com"
        },
        {
            "name": "Kevin Krummmnacker",
            "email": "kevin.krummnacker@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0|~6.0"
    },
    "autoload": {
        "psr-4": {
            "DarthSoup\\Cart\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "DarthSoup\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "DarthSoup\\Cart\\Facades\\Cart"
            }
        }
    }
}
