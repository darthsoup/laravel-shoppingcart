{
    "name": "darthsoup/shoppingcart",
    "description": "Laravel 5 Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Krummmnacker",
            "email": "kevin.krummnacker@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "~6.0|~7.0|~8.0",
        "illuminate/session": "~6.0|~7.0|~8.0",
        "illuminate/events": "~6.0|~7.0|~8.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "orchestra/testbench": "5.x"
    },
    "autoload": {
        "psr-4": {
            "DarthSoup\\Cart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarthSoup\\Tests\\Cart\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "DarthSoup\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "DarthSoup\\Cart\\Facades\\Cart"
            }
        }
    }
}
